    -webkit-transform: translate(-50%, -50%);
    content: url("/img/play.svg");
    font-size: 50px;
    opacity: 0.8;
    text-shadow: 0px 0px 30px rgba(0, 0, 0, 0.5);
    z-index: 1; }

/* End Gallery */
/* Contact Bubble */
.speech-bubble {
  position: relative;
  text-align: center;
  margin: auto;
  max-width: 250px;
  background: #000;
  border-radius: 0.4em;
  z-index: 10; }
  .speech-bubble :hover {
    color: #fff; }
  .speech-bubble :visited {
    color: #fff; }
  .speech-bubble :active {
    color: #fff; }

.speech-bubble:after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 0;
  height: 0;
  border: 1em solid transparent;
  border-top-color: #000;
  border-bottom: 0;
  border-left: 0;
  margin-left: -0.5em;
  margin-bottom: -1em; }

/* End Contact Bubble */
