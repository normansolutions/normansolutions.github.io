/* Gallery */
.gallery {
  display: flex;
  flex-wrap: wrap;
  justify-content: center; }

.video-thumbnail {
  position: relative;
  display: inline-block;
  cursor: pointer;
  margin: 0px 30px 5px 30px;
  max-width: 250px; }
  .video-thumbnail img {
    box-shadow: 10px 10px 42px -4px rgba(0, 0, 0, 0.75); }
  .video-thumbnail:before {
    position: absolute;
    top: 55%;
    left: 50%;
