/*# sourceMappingURL=milligram.min.css.map */
/* Basics */
body,
html {
  height: 100%;
  background-color: #c62129;
  color: #fff;
  text-size-adjust: none; }

.ns-h1 {
  font-size: 2em; }

.ns-h4 {
  color: #000; }

.button,
button,
input[type="button"],
input[type="reset"],
input[type="submit"] {
  background-color: #000;
  border: 0.1rem solid #fff; }

a {
  color: #d9d9d9; }

a:hover {
  color: white; }

a:visited {
  color: #d9d9d9; }

/* End Basics */
/* Image Animation */
.notloaded {
  opacity: 0;
  transform: scale(0.9); }

.loaded {
  background: none;
  opacity: 1;
  transform: scale(1);
  transition: all 400ms ease-in; }
  @media screen and (prefers-reduced-motion: reduce) {
    .loaded {
      transition: none; } }
/* End Image Animation */
/* Start Menu */
.pie {
  -webkit-tap-highlight-color: transparent;
  background: #000;
  border-radius: 50%;
  box-shadow: 0 0 4px 5px rgba(0, 0, 0, 0.2);
  cursor: pointer;
  height: 400px;
  left: -200px;
  position: absolute;
  top: -200px;
  width: 400px;
  transform: translateX(-200px) translateY(-200px);
  transition: transform 300ms;
  z-index: 1; }

.pie-color:hover {
  opacity: 0.85; }

.pie-color:active {
  opacity: 0.7; }

.pie1 {
  clip-path: polygon(200px 200px, 344px 450px, 0 450px);
  transition-delay: 30ms; }

.pie2 {
  clip-path: polygon(200px 200px, 344px 450px, 450px 344px);
  transition-delay: 60ms; }

.pie3 {
  clip-path: polygon(200px 200px, 450px 0, 450px 344px);
  transition-delay: 90ms; }

.pie-color {
  width: 100%;
  height: 100%;
  border-radius: 50%; }

.pie-color-hyperlink {
  font-size: 1.6rem;
  letter-spacing: 0.03em;
  text-shadow: 1px 1px #000;
  font-weight: 700;
  color: #fff; }

.pie-color1 {
  background: linear-gradient(135deg, #c62129, #c62129);
  clip-path: polygon(200px 200px, 344px 450px, 0 450px); }

.pie-color2 {
  background: linear-gradient(135deg, #fff, #fff);
  clip-path: polygon(200px 200px, 344px 450px, 450px 344px); }

.pie-color3 {
  background: linear-gradient(135deg, #c62129, #c62129);
  clip-path: polygon(200px 200px, 450px 0, 450px 344px); }

.videos {
  left: 208px;
  position: absolute;
  top: 320px;
  width: 46px;
  transform: rotate(77deg); }

.gigging {
  left: 272px;
  position: absolute;
  top: 279px;
  width: 46px;
  transform: rotate(46deg);
  color: #c62129; }

.contact {
  left: 304px;
  position: absolute;
  top: 220px;
  width: 46px;
  transform: rotate(16deg); }

.menu {
  -webkit-tap-highlight-color: transparent;
  background: #fff;
  border-radius: 50%;
  box-shadow: 0 0 4px 5px rgba(0, 0, 0, 0.2);
  cursor: pointer;
  height: 200px;
  left: -100px;
  position: absolute;
  top: -100px;
  width: 200px;
  z-index: 1; }

.hamburger {
  cursor: pointer;
  height: 46px;
  left: 58%;
  position: relative;
  top: 58%;
  width: 46px; }

.hamburger path {
  transition: transform 300ms; }

.hamburger path:nth-child(1) {
  transform-origin: 25% 29%; }

.hamburger path:nth-child(2) {
  transform-origin: 50% 50%; }

.hamburger path:nth-child(3) {
  transform-origin: 75% 72%; }

.hamburger path:nth-child(4) {
  transform-origin: 75% 29%; }

.hamburger path:nth-child(5) {
  transform-origin: 25% 72%; }

.active .pie {
  transform: translateX(0) translateY(0); }

.active .hamburger path:nth-child(1) {
  transform: rotate(45deg); }

.active .hamburger path:nth-child(2) {
  transform: scaleX(0); }

.active .hamburger path:nth-child(3) {
  transform: rotate(45deg); }

.active .hamburger path:nth-child(4) {
  transform: rotate(-45deg); }

.active .hamburger path:nth-child(5) {
  transform: rotate(-45deg); }

/* End Menu */
/* Header*/
/* The hero image */
.hero-image {
  /* Use "linear-gradient" to add a darken background effect to the image (photographer.jpg). This will make the text easier to read */
  background-image: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.5)), url("/img/BandHeader.jpg?v=fingerprint");
  /* Set a specific height */
  height: 50%;
  /* Position and center the image to scale nicely on all screens */
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  position: relative; }

.webp .hero-image {
  /* Use "linear-gradient" to add a darken background effect to the image (photographer.jpg). This will make the text easier to read */
  background-image: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.5)), url("/img/BandHeader.webp?v=fingerprint");
  /* Set a specific height */
  height: 50%;
  /* Position and center the image to scale nicely on all screens */
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  position: relative; }

.avif .hero-image {
  /* Use "linear-gradient" to add a darken background effect to the image (photographer.jpg). This will make the text easier to read */
  background-image: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.5)), url("/img/BandHeader.avif?v=fingerprint");
  /* Set a specific height */
  height: 50%;
  /* Position and center the image to scale nicely on all screens */
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  position: relative; }

/* Place text in the middle of the image */
.hero-text {
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%); }
  @media (min-width: 1367px) {
    .hero-text {
      width: 40%; } }
@media (min-width: 1024px) and (max-width: 1366px) and (orientation: portrait) {
  .hero-text {
    width: 80%; } }

@media (min-width: 1024px) and (max-width: 1366px) and (orientation: landscape) {
  .hero-text {
    width: 40%; } }

@media (max-width: 1023px) and (orientation: portrait) {
  .hero-text {
    width: 80%; } }

@media (max-width: 1023px) and (orientation: landscape) {
  .hero-text {
    width: 50%; } }

